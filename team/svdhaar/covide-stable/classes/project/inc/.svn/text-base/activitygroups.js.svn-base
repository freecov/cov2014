function save_activitygroup(id) {
	document.getElementById('activitygroup_'+id).submit();
}

function remove_activitygroup(id) {
	if (confirm(gettext("Are you sure you want to delete this activitygroup?"))) {
		document.getElementById('activitygroup_'+id).subaction.value='delete';
		document.getElementById('activitygroup_'+id).submit();
	}
}
