body, a, td, span, div, p, h1, h2, h3, h4, h5, h6 {
	color: black;
	font-family: arial, serif;
	font-size: 13px;
}
a {
	text-decoration: underline;
}
h1 {
	font-size: 22px;
	font-weight: bold;
}
@media print {
	.noprint {
		display: none;
	}
}