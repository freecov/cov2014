function getDiallingCode(countryCode) {
	var ret = "";
	if (diallingCodes[countryCode] != undefined) {
		ret = diallingCodes[countryCode];
	}
	return ret;
}

function updatePhoneNumbers() {
	var code = getDiallingCode($('#addresscountry').val());

	//Only update if no phone number was entered.... (max 3 chars dialing code and a +)
	if ($('#addressphone_nr').val().length <= 4) {
		$('#addressphone_nr').val("+"+code);
	}
	if ($('#addressmobile_nr').val().length <= 4) {
		$('#addressmobile_nr').val("+"+code);
	}
	if ($('#addressfax_nr').val().length <= 4) {
		$('#addressfax_nr').val("+"+code);
	}
}